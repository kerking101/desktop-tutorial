void setup() {
  int mijnEersteVariabele = 10;
  
  mijnEersteVariabele *= 10;
  println(mijnEersteVariabele);  // Output: 100
  
  mijnEersteVariabele = 10;
  mijnEersteVariabele /= 10;
  println(mijnEersteVariabele);  // Output: 1
  
  mijnEersteVariabele = 10;
  mijnEersteVariabele -= 10;
  println(mijnEersteVariabele);  // Output: 0
}
